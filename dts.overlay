/ {
	chosen {
		zephyr,shell-uart = &cdc_acm_uart0;
	};
};

&zephyr_udc0 {
	cdc_acm_uart0: cdc_acm_uart0 {
		compatible = "zephyr,cdc-acm-uart";
	};
};

&usart2 {
	status = "disabled";
};
&usart3 {
	status = "disabled";
};
&i2c1 {
	status = "disabled";
};
&i2c2 {
	status = "disabled";
};
&spi1 {
	status = "disabled";
};
&spi2 {
	status = "disabled";
};
&adc1 {
	status = "disabled";
};
